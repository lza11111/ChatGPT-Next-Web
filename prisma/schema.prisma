generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "mongodb"
  url      = env("DATABASE_URL")
}

model balance {
  id           String  @id @default(auto()) @map("_id") @db.ObjectId
  api_key      String
  disabled     Boolean
  endpoint     String
  path         String
  remain_token Int
  token        String
}
