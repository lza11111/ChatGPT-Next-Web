generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "mongodb"
  url      = env("DATABASE_URL")
}

model balance {
  id           String  @id @default(auto()) @map("_id") @db.ObjectId
  account_key  String
  account_name String
  disabled     Boolean
  endpoint     String
  path         String
  used         Int
  subscription Int
  api_key      String
}
